  </head>
  <body>
    <header>
      <% include partials/_header %>
    </header>
    <div class="bigMargin">
      <table>
      <% for (let url in urls) { %>
        <tr>
          <td><%= url %></td><td>â¤‘</td><td><%= urls[url].URL %></td>
